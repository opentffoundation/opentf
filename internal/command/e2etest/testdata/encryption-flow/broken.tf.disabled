terraform {
  encryption {
    key_provider "pbkdf2" "basic" {
      passphrase    = "aaaaaaaa-83f1-47ec-9b2d-2aebf6417167"
      key_length    = 32
      iterations    = 200000
      hash_function = "sha512"
      salt_length   = 12
    }
    method "aes_gcm" "example" {
      keys = key_provider.pbkdf2.basic
    }
    state {
      method = method.aes_gcm.example
    }
    plan {
      method = method.aes_gcm.example
    }
  }
}
